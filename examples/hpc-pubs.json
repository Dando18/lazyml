{
    "data": {
        "train": "./datasets/pub-data.csv",
        "test_split": "0.1"
    },
    "preprocess": [
        {"name": "minmax_scale", "columns": ["year", "referenceCount", "influentialCitationCount"]}
    ],
    "train": {
        "task": "regression",
        "X": {"columns-regex": "embedding_[\\d]+"},
        "y": {"columns": ["citationCount"]},
        "metrics": ["accuracy", "f1"],
        "models": ["XGB", "AdaBoost", "Dummy"]
    }
}
